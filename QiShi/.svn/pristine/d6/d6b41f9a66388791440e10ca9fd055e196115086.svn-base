'use strict';

module.exports = {
    createImage: function createImage(url, node) {
        var self = this;
        if (url) {
            cc.loader.load({
                url: url, type: 'png'
            }, function (err, texture) {
                if (!err && self) {
                    var sp = node;
                    sp.spriteFrame = new cc.SpriteFrame(texture);
                    sp.type = cc.Sprite.Type.SIMPLE;
                    sp.sizeMode = cc.Sprite.SizeMode.CUSTOM;
                }
            });
        }
    }
};

