@charset "utf-8";
/*-------------reset------------*/
body,h1,h2,h3,h4,h5,h6,hr,p,dl,dt,dd,ul,ol,li,pre,code,form,fieldset,legend,button,input,textarea,blockquote,th,td{margin:0;padding:0;}
ol,ul,li{list-style:none;}
em{ font-style:normal}
img{border:none}
textarea{word-break:break-all;word-wrap:break-word;resize:none;font-family:"微软雅黑"; overflow:auto}
input{font-family:"微软雅黑";}
*:focus {outline: none;} 

/*-------------end reset------------*/

/* global element */
