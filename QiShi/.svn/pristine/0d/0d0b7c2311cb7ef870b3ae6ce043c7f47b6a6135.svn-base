/*-------------二维码开始------------*/

.login-tip {position: absolute;right: 60px;top: 10px;z-index: 100;}
.poptip {line-height: 16px;background: #fefcee none repeat scroll 0 0;border: 1px solid #f3d995;height: 16px;line-height: 16px;padding: 5px 20px 5px 15px;position: relative;border-radius: 6px;}
.poptip .poptip-content {color: #df9c1f;font-size: 12px;font-weight: 400;}
.poptip-arrow {right: 0;top: 8px;position: absolute;z-index: 10;}
.poptip-arrow em {border-left-color: #f3d995;border-width: 6px 0 6px 6px;left: 1px;top: 0;}
.poptip-arrow em, .poptip-arrow span {border-color: rgba(255, 255, 255, 0);border-style: solid;height: 0;left: 0;overflow: hidden;position: absolute;top: 0;width: 0;}
.poptip-arrow span {border-left-color: #fefcee;border-width: 6px 0 6px 6px;}

/*-------------二维码结束------------*/

